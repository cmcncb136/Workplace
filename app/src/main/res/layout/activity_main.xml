<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".MainActivity">


    <Button
            android:id="@+id/loginGoBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Login Page 이동"
            />

    <Button
            android:id="@+id/mainGoBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Home Page"
            />

    <Button
            android:id="@+id/joinBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="JOIN Page"
    />

    <Button
            android:id="@+id/productsBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Products Page"
    />

    <Button
            android:id="@+id/ProductDetailBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Products detail Page"
    />

    <Button
            android:id="@+id/ProductBuyBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Products Buy Page"
    />

    <Button
            android:id="@+id/BuyCompleteBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Buy complete Page"
    />


</LinearLayout>